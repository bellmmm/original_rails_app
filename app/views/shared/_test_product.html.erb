<div id="test_product-<%= test_product.id%>">
  <span class="test_product-image">
        <% if File.exist?"app/assets/images/products/#{test_product.name}.jpg" %>
          <div class="test_product_image">
            <%= image_tag("products/#{test_product.name}.jpg", alt: "products/#{test_product.name}.jpg", width: "200px") %>
            <p class="photographer">Photo by: <%=test_product.photo_by%></p>
            
          </div> 
        <% else %>
        <% end %>
  </span>
  <div id="dislike_button-<%= test_product.id%>">
    <% if current_user.setting_dislike?(test_product)%>
      <%= render partial: 'shared/unset_dislike', locals: {test_product: test_product}  %>
    <% else %> 
      <%= render partial: 'shared/set_dislike', locals: {test_product: test_product}  %>
    <% end %> 
  </div>
</div>

  

